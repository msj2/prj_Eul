import math
#math.sqrt(x)

def is_prime(prime):
	n_max = int(math.sqrt(prime))
	#print "Module is_prime.......Checking if", prime, "is a prime no.... "
	n_max += 1
	for n in range(3, n_max, 2):
		if ( prime % n == 0 ):
	            return 0
        	else:
            		continue
	return 1

i = 6
n = 15
while ( i < 1002):
	if ( is_prime( n) == 1):
		print n, " is ",i,"'th prime no. "
		i = i + 1
	n = n + 2	
